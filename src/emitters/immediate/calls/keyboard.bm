'keyboard.bm - Executors for keyboard handling functions

case TOK_INKEY
    result.s = inkey$

case TOK__KEYCLEAR
    _keyclear

case TOK__KEYDOWN
    imm_eval ast_get_child(node, 1), v1
    result.n = _keydown(v1.n)

case TOK__KEYHIT
    result.n = _keyhit
